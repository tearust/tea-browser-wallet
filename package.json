    "element-ui": "2.13.2",
    "js-yaml": "^3.14.0",
    "jsoneditor": "9.5.0",
    "lodash": "4.17.21",
    "protobufjs": "^6.9.0",
    "pubsub-js": "^1.9.0",
    "register-service-worker": "^1.6.2",
    "tearust_layer1": "1.0.1",
    "tearust_utils": "1.0.1",
    "vue": "2.6.14",
